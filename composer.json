{
    "name": "caashapp/plaid-sdk-laravel",
    "description": "Laravel implementation of Plaid's API as an SDK.",
    "license": "MIT",
    "authors": [
        {
            "name": "Kyle Klaus",
            "email": "kklaus@indemnity83.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/http": "^8.79|^9.0",
        "illuminate/support": "^8.79|^9.0",
        "moneyphp/money": "^3.0",
        "spatie/data-transfer-object": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "CaashApp\\Plaid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CaashApp\\Plaid\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CaashApp\\Plaid\\PlaidServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
